{
  "name": "no-deps-in-source",
  "version": "1.0.0",
  "license": "MIT",
  "esy": {
    "buildsInSource": "true",
    "build": [
      "echo '#!/bin/bash\necho $cur__name' > ./$cur__name",
      "chmod +x ./$cur__name"
    ],
    "install": [
      "cp ./$cur__name $cur__bin/$cur__name"
    ]
  }
}
