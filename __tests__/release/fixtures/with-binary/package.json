{
  "name": "with-binary",
  "description": "simple package with a binary",
  "version": "0.1.0",
  "license": "MIT",
  "esy": {
    "build": [
      [
        "cp",
        "./say-hello.exe",
        "$cur__install/bin/say-hello.exe"
      ],
      [
        "chmod",
        "+x",
        "$cur__install/bin/say-hello.exe"
      ]
    ],
    "release": {
      "releasedBinaries": [
        "say-hello.exe"
      ]
    }
  }
}
