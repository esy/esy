{
  "name": "esy",
  "version": "0.0.1",
  "description": "Package builder for esy",
  "license": "MIT",
  "esy": {
    "build": [
      [
        "jbuilder",
        "build",
        "-j",
        "4"
      ],
      [
        "jbuilder",
        "build",
        "esy/bin/esyCommand.exe",
        "esy-build-package/bin/esyBuildPackageCommand.exe"
      ]
    ],
    "install": [
      [
        "esy-installer",
        "./esy-build-package.install"
      ],
      [
        "esy-installer",
        "./esy.install"
      ]
    ],
    "buildsInSource": "_build"
  },
  "dependencies": {
    "@esy-ocaml/esy-installer": "^0.0.0",
    "@opam/bos": "^0.1.6",
    "@opam/cmdliner": "^1.0.2",
    "@opam/fmt": "^0.8.4",
    "@opam/fpath": "^0.7.2",
    "@opam/jbuilder": "^1.0.0-beta18",
    "@opam/logs": "^0.6.2",
    "@opam/lwt": "^3.3.0",
    "@opam/lwt_ppx": "^1.1.0",
    "@opam/ppx_deriving": "^4.2.1",
    "@opam/ppx_deriving_yojson": "^3.1.0",
    "@opam/ppx_import": "^1.4.0",
    "@opam/ppx_inline_test": "^100000000.10.0",
    "@opam/ppx_let": "^100000000.9.0",
    "@opam/re": "^1.7.1",
    "@esy-ocaml/reason": "^3.1.0",
    "fastreplacestring": "IwanKaramazow/FastReplaceString.git#0.0.4",
    "ocamlstripdebug": "andreypopp/ocamlstripdebug#de74dde"
  },
  "peerDependencies": {
    "ocaml": "~4.6.0"
  },
  "devDependencies": {
    "@esy-ocaml/merlin": "*",
    "@opam/ocp-indent": "*",
    "ocaml": "~4.6.0"
  }
}
