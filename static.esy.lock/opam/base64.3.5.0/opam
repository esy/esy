  "ocaml" {>= "4.03.0"}
  "dune" {>= "2.0"}
  "bos" {with-test}
  "rresult" {with-test}
  "alcotest" {with-test}
]
build: [
  ["dune" "subst"] {dev}
  ["dune" "build" "-p" name "-j" jobs]
  ["dune" "runtest" "-p" name "-j" jobs] {with-test}
]
