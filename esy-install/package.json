{
  "name": "esy",
  "version": "0.1.7",
  "description": "Esy sandboxes for compiled languages",
  "main": "lib/index.js",
  "scripts": {
    "test": "make ci"
  },
  "bin": {
    "esy": "./bin/esy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esy/esy.git"
  },
  "keywords": ["esy", "ocaml", "sandbox"],
  "jest": {
    "timers": "fake",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/__tests__/fixtures/",
      "<rootDir>/__tests__/build/fixtures",
      "<rootDir>/yarn-install",
      "<rootDir>/dist",
      "<rootDir>/esyInstallCache-3.x.x"
    ],
    "testPathIgnorePatterns": [
      "__tests__/(fixtures|__mocks__)/",
      "<rootDir>/yarn",
      "<rootDir>/dist",
      "<rootDir>/esyInstallCache-3.x.x",
      "<rootDir>/lib"
    ],
    "transformIgnorePatterns": ["<rootDir>/node_modules/(?!@esy-ocaml/yarn)"]
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/esy/esy/issues"
  },
  "homepage": "https://github.com/esy/esy#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^22.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-inline-imports-commonjs": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015-node4": "^2.1.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-node5": "^12.0.1",
    "babel-preset-stage-0": "^6.0.0",
    "babel-register": "^6.24.1",
    "doctoc": "^1.3.0",
    "flow-bin": "^0.60.0",
    "is-ci": "^1.0.10",
    "jest": "^21.0.0",
    "prettier": "^1.7.4",
    "raw-loader": "^0.5.1",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "@esy-ocaml/esy-install": "link:./esy-install",
    "@esy-ocaml/esy-opam": "0.1.0",
    "babel-polyfill": "^6.23.0",
    "cli-argparse": "^1.1.2",
    "debug": "^3.1.0",
    "fs-extra": "^3.0.1",
    "json-stable-stringify": "https://github.com/substack/json-stable-stringify",
    "json5": "^0.5.1",
    "loud-rejection": "^1.6.0",
    "mkdirp": "^0.5.1",
    "npm-hooks": "https://github.com/jordwalke/npm-hooks.git#1.0.0-clobber",
    "outdent": "^0.3.0",
    "p-queue": "^1.0.0",
    "resolve": "^1.5.0",
    "rimraf": "^2.6.1",
    "shell-escape": "^0.2.0",
    "shellwords": "^0.1.1",
    "var-expansion": "^0.1.0",
    "walkdir": "^0.0.11"
  }
}
