build: [
  ["dune" "subst"] {pinned}
  [
    "dune"
    "build"
    "-p"
    name
    "-j"
    jobs
    "@install"
    "@runtest" {with-test}
    "@doc" {with-doc}
  ]
]
dev-repo: "git+https://github.com/esy/esy.git"
pin-depends: [
  ["camlbz2.dev" "git+https://gitlab.com/irill/camlbz2.git#588e186c"]
  ["bos.dev" "git+https://github.com/esy-ocaml/bos.git#90364d00"]
